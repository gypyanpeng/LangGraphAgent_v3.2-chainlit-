{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "transport": "stdio"
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.4"
      ],
      "env": {
        "TAVILY_API_KEY": "your_tavily_key"
      },
      "transport": "stdio"
    },
   "mcp-server-chart": {
      "command": "npx",
      "args": [
        "-y",
        "@antv/mcp-server-chart"
      ],
      "transport": "stdio"
    }
  }
}